{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "baseUrl": "./src",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "core/*": ["core/*"],
      "hooks/*": ["hooks/*"],
      "modules/*": ["modules/*"],
      "test/*": ["test/*"],
      "logomaker/*": ["logomaker/*"],
      "images/*": ["images/*"]
    }
  },
  "include": [
    "src/**/*", "test/*"
  ]
}